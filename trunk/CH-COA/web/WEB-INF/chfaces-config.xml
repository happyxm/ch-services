<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<!-- head -->
	<component>
		<component-type>com.chfaces.HtmlOutputHead</component-type>
		<component-class>com.chfaces.custom.outputhead.OutputHead</component-class>
	</component>
	<!-- body -->
	<component>
		<component-type>com.chfaces.HtmlCHHtmlBody</component-type>
		<component-class>com.chfaces.custom.htmlbody.CHHtmlBody</component-class>
	</component>
	<!-- div -->
	<component>
		<component-type>com.chfaces.HtmlDiv</component-type>
		<component-class>com.chfaces.custom.html.HtmlDiv</component-class>
	</component>
	<!-- ajaxEvent -->
	<component>
		<component-type>com.chfaces.HtmlAjaxEvent</component-type>
		<component-class>com.chfaces.custom.ajax.AjaxEvent</component-class>
	</component>
	<!-- ajaxStatus -->
	<component>
		<component-type>com.chfaces.HtmlAjaxStatus</component-type>
		<component-class>com.chfaces.custom.ajax.AjaxStatus</component-class>
	</component>
	<!-- simpleTree -->
	<component>
		<component-type>com.chfaces.HtmlSimpleTree</component-type>
		<component-class>com.chfaces.custom.simpletree.SimpleTree</component-class>
	</component>
	<!-- button -->
	<component>
		<component-type>com.chfaces.HtmlButton</component-type>
		<component-class>com.chfaces.custom.button.Button</component-class>
	</component>
	<!-- dialog -->
	<component>
		<component-type>com.chfaces.HtmlDialog</component-type>
		<component-class>com.chfaces.custom.dialog.Dialog</component-class>
	</component>
	<!-- paginator -->
	<component>
		<component-type>com.chfaces.HtmlPaginator</component-type>
		<component-class>com.chfaces.custom.paginator.Paginator</component-class>
	</component>
	<!-- tabGroup -->
	<component>
		<component-type>com.chfaces.HtmlTabGroup</component-type>
		<component-class>com.chfaces.custom.tab.TabGroup</component-class>
	</component>
	<!-- tab -->
	<component>
		<component-type>com.chfaces.HtmlTab</component-type>
		<component-class>com.chfaces.custom.tab.Tab</component-class>
	</component>
	<!-- multiSelect -->
	<component>
		<component-type>com.chfaces.HtmlMultiSelect</component-type>
		<component-class>com.chfaces.custom.multiselect.MultiSelect</component-class>
	</component>
	<!-- datagrid -->
	<component>
		<component-type>com.chfaces.HtmlDataGrid</component-type>
		<component-class>com.chfaces.custom.datagrid.DataGrid</component-class>
	</component>
	<!-- datepicker -->
	<component>
		<component-type>com.chfaces.HtmlDatePicker</component-type>
		<component-class>com.chfaces.custom.datepicker.DatePicker</component-class>
	</component>
	<!-- select -->
	<component>
		<component-type>com.chfaces.HtmlSelectOneListbox</component-type>
		<component-class>com.chfaces.custom.htmlbasic.SelectOneListbox</component-class>
	</component>
	<!-- radio -->
	<component>
		<component-type>com.chfaces.HtmlSelectOneRadio</component-type>
		<component-class>com.chfaces.custom.htmlbasic.SelectOneRadio</component-class>
	</component>
	<!-- listToList -->
	<component>
		<component-type>com.chfaces.HtmlListToList</component-type>
		<component-class>com.chfaces.custom.listtolist.ListToList</component-class>
	</component>
	<!-- upload -->
	<component>
		<component-type>com.chfaces.HtmlFileUploader</component-type>
		<component-class>com.chfaces.custom.fileuploader.FileUploader</component-class>
	</component>
	
	<render-kit>
		<!-- head -->
		<renderer>
			<component-family>com.chfaces.OutputHead</component-family>
			<renderer-type>com.chfaces.OutputHeadRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.outputhead.OutputHeadRenderer</renderer-class>
		</renderer>
		<!-- body -->
		<renderer>
			<component-family>com.chfaces.CHHtmlBody</component-family>
			<renderer-type>com.chfaces.CHHtmlBodyRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.htmlbody.CHHtmlBodyRenderer</renderer-class>
		</renderer>
		<!-- div -->
		<renderer>
			<component-family>com.chfaces.Div</component-family>
			<renderer-type>com.chfaces.DivRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.html.HtmlDivRenderer</renderer-class>
		</renderer>
		<!-- ajaxEvent -->
		<renderer>
			<component-family>com.chfaces.AjaxEvent</component-family>
			<renderer-type>com.chfaces.AjaxEventRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.ajax.AjaxEventRenderer</renderer-class>
		</renderer>
		<!-- ajaxStatus -->
		<renderer>
			<component-family>com.chfaces.AjaxStatus</component-family>
			<renderer-type>com.chfaces.AjaxStatusRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.ajax.AjaxStatusRenderer</renderer-class>
		</renderer>
		<!-- simpleTree -->
		<renderer>
			<component-family>com.chfaces.SimpleTree</component-family>
			<renderer-type>com.chfaces.SimpleTreeRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.simpletree.SimpleTreeRenderer</renderer-class>
		</renderer>
		<!-- button -->
		<renderer>
			<component-family>com.chfaces.Button</component-family>
			<renderer-type>com.chfaces.ButtonRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.button.ButtonRenderer</renderer-class>
		</renderer>
		<!-- dialog -->
		<renderer>
			<component-family>com.chfaces.Dialog</component-family>
			<renderer-type>com.chfaces.DialogRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.dialog.DialogRenderer</renderer-class>
		</renderer>
		<!-- paginator -->
		<renderer>
			<component-family>com.chfaces.Paginator</component-family>
			<renderer-type>com.chfaces.PaginatorRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.paginator.PaginatorRenderer</renderer-class>
		</renderer>
		<!-- tabGroup -->
		<renderer>
			<component-family>com.chfaces.TabGroupRenderer</component-family>
			<renderer-type>com.chfaces.TabGroupRendererRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.tab.TabGroupRenderer</renderer-class>
		</renderer>
		<!-- tab -->

		<!-- multiSelect -->
		<renderer>
			<component-family>com.chfaces.MultiSelect</component-family>
			<renderer-type>com.chfaces.MultiSelectRendererRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.multiselect.MultiSelectRenderer</renderer-class>
		</renderer>
		<!-- datagrid -->
		<renderer>
			<component-family>com.chfaces.DataGrid</component-family>
			<renderer-type>com.chfaces.HtmlDataGridRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.datagrid.DataGridRenderer</renderer-class>
		</renderer>
		<!-- datepicker --> 
		<renderer>
			<component-family>com.chfaces.DatePicker</component-family>
			<renderer-type>com.chfaces.DatePickerRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.datepicker.DatePickerRenderer</renderer-class>
		</renderer>
		<!-- select -->
		<renderer>
			<component-family>com.chfaces.SelectOneListbox</component-family>
			<renderer-type>com.chfaces.HtmlSelectOneListboxRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.htmlbasic.SelectOneListboxRenderer</renderer-class>
		</renderer>
		<!-- radio -->
		<renderer>
			<component-family>com.chfaces.SelectOneRadio</component-family>
			<renderer-type>com.chfaces.SelectOneRadioRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.htmlbasic.SelectOneRadioRenderer</renderer-class>
		</renderer>
		<!-- listToList -->
		<renderer>
			<component-family>com.chfaces.ListToList</component-family>
			<renderer-type>com.chfaces.ListToListRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.listtolist.ListToListRenderer</renderer-class>
		</renderer>
		<!-- upload -->
		<renderer>
			<component-family>com.chfaces.FileUploader</component-family>
			<renderer-type>com.chfaces.FileUploaderRenderer</renderer-type>
			<renderer-class>com.chfaces.custom.fileuploader.FileUploaderRenderer</renderer-class>
		</renderer>
	</render-kit>
</faces-config>